<uwsgi>    
   <socket>127.0.0.1:8008</socket> <!-- 内部端口，自定义 --> 
 
   <chdir>/www/wwwroot/djangoproject</chdir> <!-- 项目路径 -->   
         
   <module>learning_log.wsgi</module>  
          <!--wsgi前面的文件名为wsgi.py文件所在的文件名 -->
 
   <processes>4</processes> <!-- 进程数 -->     
   
   <daemonize>uwsgi.log</daemonize> <!-- 日志文件 -->
</uwsgi>